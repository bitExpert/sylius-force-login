    <class name="BitExpert\SyliusForceCustomerLoginPlugin\Model\WhitelistEntry">
        <property name="label">
            <constraint name="NotBlank">
                <option name="message">bitexpert_sylius_forcelogin.form.label.not_blank</option>
            </constraint>
        </property>
        <property name="urlRule">
            <constraint name="NotBlank">
                <option name="message">bitexpert_sylius_forcelogin.form.urlRule.not_blank</option>
            </constraint>
        </property>
    </class>
</constraint-mapping>
